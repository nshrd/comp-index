# ============================================================================
# Пример конфигурации для обновления композитного индекса рейтингов
# Скопируйте этот файл в .env и заполните своими значениями
# ============================================================================

# API настройки
APP_RANKINGS_API_KEY=your_api_key_here
API_BASE_URL=https://your-api-domain.com

# GitHub настройки (для автоматизации)
GITHUB_TOKEN=your_github_token_here
SEEDS_REPO_NAME=your_username/your_seeds_repo_name

# Настройки обработки данных
MA_LENGTH=30
MAX_RANK_VALUE=600

# Уведомления (опционально)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Настройки логирования
LOG_LEVEL=INFO
KEEP_LOGS_DAYS=30 